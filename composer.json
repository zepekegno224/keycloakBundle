{
    "$schema": "https://raw.githubusercontent.com/composer/composer/main/res/composer-schema.json",
    "name": "zepekegno/keycloak-bundle",
    "description": "Symfony bundle for Keycloak integration (OIDC, SSO, user management)",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zepekegno\\KeycloakBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zepekegno\\KeycloakBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Moussa TraorÃ©",
            "email": "moussatraore158@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/security-bundle": "^6.0",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/dom-crawler": "^6.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "symfony-bundle",
        "keycloak",
        "openid-connect",
        "oidc",
        "sso",
        "authentication"
    ],
    "homepage": "https://github.com/zepekegno224/keycloakBundle",
    "support": {
        "issues": "https://github.com/zepekegno224/keycloakBundle/issues",
        "source": "https://github.com/zepekegno224/keycloakBundle"
    }
}
